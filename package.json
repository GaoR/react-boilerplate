{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "boiler-plate for react apps",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/index.js",
    "build-dev": "webpack --watch --config webpack.dev.js",
    "build-prod": "webpack --config webpack.prod.js",
